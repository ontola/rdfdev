{
  "name": "@rdfdev/iri",
  "version": "1.2.2",
  "description": "Toolkit to make working with IRI's quick and easy",
  "scripts": {
    "build": "pika build",
    "test": "jest --coverage"
  },
  "author": "rescribet <thom@argu.co>",
  "license": "MIT",
  "repository": "https://github.com/ontola/rdfdev-js",
  "keywords": [
    "iri",
    "url",
    "namednode",
    "named",
    "node",
    "rdf",
    "rest",
    "linked data",
    "linked delta",
    "open",
    "linked",
    "data",
    "deleta",
    "semantic",
    "web",
    "link",
    "link-lib",
    "link-redux",
    "ontology",
    "ontologies",
    "triples",
    "quads",
    "n-triples",
    "turtle",
    "json-ld",
    "solid"
  ],
  "publishConfig": {
    "access": "public"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "peerDependencies": {
    "@ontologies/core": "*"
  },
  "devDependencies": {
    "@ontologies/core": "^2.0.2",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-build-web": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.8.1",
    "@types/jest": "^24.0.23",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    }
  }
}
